{
  "name": "git",
  "versions": [
    {
      "version": "2.45.0",
      "description": "Distributed version control system",
      "source": {
        "type": "tarball",
        "url": "https://www.kernel.org/pub/software/scm/git/git-2.45.0.tar.gz"
      },
      "dependencies": ["zlib", "openssl", "curl", "pcre2"],
      "build_dependencies": ["pkg-config"],
      "build_system": "autotools",
      "configure_args": [
        "--with-openssl",
        "--with-curl",
        "--with-libpcre",
        "--without-tcltk",
        "--with-zlib"
      ],
      "env": {}
    },
    {
      "version": "2.44.0",
